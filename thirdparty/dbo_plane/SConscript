Import('env')
dbo_planeenv = env.Copy()
dbo_plane_sources=Split("""
	dbo_plane.c""")

# Set any platform specific options for this target
dbo_planeenv.UseTargetOptions('plugin')
dbo_planeenv.UseTargetOptions('texture_plugin')
dbo_planeenv.UseTargetOptions('dbo_plane')

dbo_plane = dbo_planeenv.SharedLibrary('dbo_plane', dbo_plane_sources, LIBS=[''])

dbo_planeenv.Install('$BINDIR', dbo_plane)

env.Distribute('thirdparty/dbo_plane/dbo_plane', dbo_plane_sources)
env.Distribute('thirdparty/dbo_plane/dbo_plane', 'SConscript')
