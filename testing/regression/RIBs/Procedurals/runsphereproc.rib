version 3.03
Format 320 240 1
Option "searchpath" "procedural" "$PATH:&"
Display "runsphereproc.tif" "file" "rgb" "string compression" "lzw"
Projection "perspective" "fov" [50]
Translate 0 0 5
WorldBegin
Surface "constant"
TransformBegin
Color [0 1 0]
Translate -1 0 0
Procedural "RunProgram" ["python sphereproc.py" "1.0"] [-1 1 -1 1 -1 1]
TransformEnd
TransformBegin
Color [1 0 0]
Translate 1 0 0
Procedural "RunProgram" ["python sphereproc.py" "0.8"] [-0.80000000000000004 0.80000000000000004 -0.80000000000000004 0.80000000000000004 -0.80000000000000004 0.80000000000000004]
TransformEnd
WorldEnd
