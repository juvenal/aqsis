##RenderMan RIB
version 3.03
# Minus X
FrameBegin 1
	ShadingRate 2
	PixelSamples 1 1
	PixelFilter "box" 1 1
	Format 320 320 1
	Display "rnx.temp" "file" "rgba" 
	Projection "perspective" "fov" [91]
	Clipping 0.1 100000
	Rotate 90 0 1 0
	WorldBegin 
		ReadArchive "real_env_lights"
		ReadArchive "real_env_world"
	WorldEnd 
FrameEnd 
