version 3.03
Option "searchpath" "shader" ["../../shaders:&"]

Display "genpoly_test.tif" "file" "rgba" "string compression" "lzw"

Format 200 150 1
ShadingRate 2

Option "searchpath" "shader" ["./:&"]

Projection "perspective" "fov" 45
Translate 0 -1 1
Rotate -40 1 0 0

Option "limits" "eyesplits" [3]
Sides 2

WorldBegin
      
  LightSource "ambientlight" 0 "intensity" [0.1]
  LightSource "distantlight" 3 "intensity" [0.5] "lightcolor" [1 1 1] "from" [-2 4 -2] "to" [0 0 0]
  LightSource "distantlight" 4 "intensity" [0.7] "lightcolor" [1 1 1] "from" [1 2 -2] "to" [0 0 0]

  Surface "plastic"

  AttributeBegin
	Color [1 0 0]
	Translate -1 0 1

	PointsGeneralPolygons [2 2] [4 4 4 3] [0 1 2 3 4 5 6 7 3 2 8 9 10 11 12]
		"P" [
				0 0 0
				0 0 2
				1 0 2
				1 0 0
				0.25 0 0.25
				0.25 0 1.75
				0.75 0 1.75
				0.75 0 0.25
				2 0 2
				2 0 0
				1.25 0 0.25
				1.5 0 1.75
				1.75 0 0.25
			]
  AttributeEnd

  AttributeBegin
	Color [1 1 0]
	Translate -1 0 1

	GeneralPolygon [4 4]
		"P" [
				0.25 0 0.25
				0.25 0 1.75
				0.75 0 1.75
				0.75 0 0.25
				0.33 0 0.33
				0.33 0 1.66
				0.66 0 1.66
				0.66 0 0.33
			]
  AttributeEnd

WorldEnd

