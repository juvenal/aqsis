version 3.03
Option "searchpath" "shader" ["../../shaders:&"]

Display "Bug1184401.tif" "file" "rgba" "string compression" "deflate"

Format 200 150 1
ShadingRate 2

Option "searchpath" "shader" ["./:&"]

Projection "perspective" "fov" 50
Translate 0 -1 1
Rotate -40 1 0 0

Option "limits" "eyesplits" [3]
Sides 2

WorldBegin
      
  AttributeBegin
	Color [1 0 0]
	PointsPolygons [3 3] [0 1 2  0 2 3]
		"vertex point P" [
				0 0 0
				0 0 2
				1 0 2
				1 0 0
			]
  AttributeEnd

WorldEnd

