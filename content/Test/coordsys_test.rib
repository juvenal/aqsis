version 3.03
Display "coordsys_test.tif" "file" "rgba"
#Display "coordsys_test.z" "zfile" "z"
Format 200 150 1

#Clipping 1e-05 1000000
#Clipping 5 10

Projection "perspective" "fov" 45
#Rotate -40 1 0 0
#Translate 0 -4 5
Translate 0 0 5

Option "limits" "eyesplits" 3
ShadingRate 2
Sides 2

WorldBegin
      
	LightSource "ambientlight" 0 "intensity" [1]

	Color [1 1 0]
	Surface "matte"

	Translate 2 0 0
	Sphere 1 -1 1 360
	CoordinateSystem "def"

	Translate -2 0 0
	Color [1 0 0]
	Sphere 1 -1 1 360

	CoordSysTransform "def"
	Color [0 1 0]
	Cylinder 0.5 -2 2 360

WorldEnd

