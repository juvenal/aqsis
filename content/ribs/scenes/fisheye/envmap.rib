# Minus X
FrameBegin 1
	ShadingRate 2
	PixelSamples 2 2
	PixelFilter "box" 1 1
	Format 320 320 1
	Display "rnx.tif" "file" "rgba" 
	Projection "perspective" "fov" [93]
	Clipping 0.1 100000
	Rotate 90 0 1 0
	WorldBegin
		Rotate 45 0 1 0 
		ReadArchive "lights.rib"
		ReadArchive "world.rib"
	WorldEnd 
FrameEnd 
