Import('env')
raytrace_sources = Split("""
	raytrace.cpp""")

raytrace_headers = Split("""
	raytrace.h""")

raytraceenv = env.Copy()

# Set any platform specific options for this target
raytraceenv.UseTargetOptions('raytrace')

raytrace = raytraceenv.Library('raytrace', raytrace_sources)
raytraceenv.Install('$STATICLIBDIR', raytrace)

env.Distribute('renderer/raytrace', raytrace_sources)
env.Distribute('renderer/raytrace', raytrace_headers)
env.Distribute('renderer/raytrace', 'SConscript')

